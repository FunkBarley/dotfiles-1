[user]
  name = George Gritsouk
  email = george.gritsouk@gmail.com
[alias]
  unstage = reset HEAD --
  discard = checkout --
  nevermind = !git reset --hard HEAD && git clean -d -f
  point = reset --soft
  uncommit = reset --mixed HEAD~
  save = commit -m
  invert = revert
  last = log -1 HEAD --format=format:"%Cred%H"
  summary = status -u -s
  graph = log --graph -10 --format=format:'%Cgreen%h %Cresetâ€¢ %s (%cr by %cn) %Cred%d' --author-date-order
  jira = log --format=format:'%cn committed %h %ai \n%s\n' --grep
  branchout = checkout -b
  branches = branch -a
  precommit = diff --cached --diff-algorithm=minimal -w
  move = mv
  remove = rm
  unmerged = branch --no-merged
[core]
  editor = vim
  precomposeunicode = true
[credential]
  helper = osxkeychain
